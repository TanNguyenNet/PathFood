@model IEnumerable<GroupQuestionModel>
<!-- QA  -->
<h1>123132132132</h1>
<div class="box">
    <div class="QA-list">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-md-12">
                    <div class="qa-wrap">
                        <ul>
                            <li class="active" value="all">All</li>
                            @foreach (var item in Model)
                            {
                                <li value="@item.Slug">@item.Name</li>
                            }
                        </ul>
                    </div>
                </div>
                <div class="col-md-10">
                    <div class="QA-content">
                        <div class="accordion_container">
                            @foreach (var group in Model)
                            {
                                foreach (var question in group.Question)
                                {
                                    <div class="wrap" data-filter="@group.Slug">
                                        <div class="accordion_head"><span class="plusminus">+</span>@question.Name</div>
                                        <div class="accordion_body" style="display: none;">
                                            <p>@question.Anwser</p>
                                        </div>
                                    </div>
                                }
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
